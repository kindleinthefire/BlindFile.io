{
  "name": "blind-file",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "worker:dev": "wrangler dev --config wrangler.toml",
    "worker:deploy": "wrangler deploy --config wrangler.toml",
    "db:migrate": "wrangler d1 execute blind-file-db --file=./schema.sql"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.22.0",
    "zustand": "^4.5.0",
    "framer-motion": "^11.0.0",
    "lucide-react": "^0.330.0"
  },
  "devDependencies": {
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.0",
    "autoprefixer": "^10.4.17",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.4.0",
    "vite": "^5.4.0",
    "@cloudflare/workers-types": "^4.20240117.0",
    "wrangler": "^3.28.0",
    "hono": "^4.0.0"
  }
}
