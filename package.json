{
    "name": "blind-file",
    "private": true,
    "version": "1.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "tsc -p tsconfig.app.json && vite build",
        "lint": "eslint .",
        "preview": "vite preview",
        "pages:dev": "wrangler pages dev dist --compatibility-date=2024-01-01",
        "pages:deploy": "wrangler pages deploy dist",
        "db:migrate": "wrangler d1 execute blind-file-db --file=./schema.sql"
    },
    "dependencies": {
        "@react-three/drei": "^10.7.7",
        "@react-three/fiber": "^8.18.0",
        "framer-motion": "^11.0.0",
        "hono": "^4.0.0",
        "lucide-react": "^0.330.0",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-router-dom": "^6.22.0",
        "three": "^0.182.0",
        "zustand": "^4.5.0"
    },
    "devDependencies": {
        "@cloudflare/workers-types": "^4.20240117.0",
        "@types/react": "^18.3.0",
        "@types/react-dom": "^18.3.0",
        "@vitejs/plugin-react": "^4.3.0",
        "autoprefixer": "^10.4.17",
        "postcss": "^8.4.35",
        "tailwindcss": "^3.4.1",
        "typescript": "^5.4.0",
        "vite": "^5.4.0",
        "wrangler": "^3.28.0"
    }
